{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "lib": [ "es6" ],
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules"
  ]
}
